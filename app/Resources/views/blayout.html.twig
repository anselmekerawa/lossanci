<!DOCTYPE html>
<html lang="en">

<head>
    {% block meta %}
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {% endblock %}

    {% block title %}
    <title>Lossanci Control Panel | </title>
    {% endblock %}

    <!-- Bootstrap core CSS -->
    {% block css %}
    <link href="{{ asset('css/bootstrap.min.css')}}" rel="stylesheet">

    <link href="{{ asset('fonts/back-end/css/font-awesome.min.css') }}" rel="stylesheet">
    <link href="{{ asset('css/back-end/animate.min.css') }}" rel="stylesheet">

    <!-- Custom styling plus plugins -->
    <link href="{{ asset('css/back-end/custom.css') }}" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="{{ asset('css/maps/jquery-jvectormap-2.0.3.css') }}" />
    <link href="{{ asset('css/back-end/icheck/flat/green.css') }}" rel="stylesheet" />
    <link href="{{ asset('css/back-end/floatexamples.css') }}" rel="stylesheet" type="text/css" />
    {% endblock %}

    {% block headjs %}
    <script src="{{ asset('js/jquery.min.js') }}"></script>
    <script src="{{ asset('js/back-end/nprogress.js') }}"></script>

    <!--[if lt IE 9]>
    <script src="{{ asset('../assets/js/ie8-responsive-file-warning.js') }}"></script>
    <![endif]-->

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    {% endblock %}

</head>


<body class="nav-md">

<div class="container body">


    <div class="main_container">

        {% block left_nav %}
            {% include('back-end/inc/left_nav.twig') %}
        {% endblock %}


        {# top_nav #}
        {% block top_nav %}
        <!-- top navigation -->
            {% include('back-end/inc/top_nav.html.twig') %}
        <!-- /top navigation -->
        {% endblock %}

        {% block container %}
        {% endblock %}

    </div>

</div>

<div id="custom_notifications" class="custom-notifications dsp_none">
    <ul class="list-unstyled notifications clearfix" data-tabbed_notifications="notif-group">
    </ul>
    <div class="clearfix"></div>
    <div id="notif-group" class="tabbed_notifications"></div>
</div>

<script src="{{ asset('js/bootstrap.min.js') }}"></script>

<!-- gauge js -->
<script type="text/javascript" src="{{ asset('js/back-end/gauge/gauge.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('js/back-end/gauge/gauge_demo.js') }}"></script>
<!-- bootstrap progress js -->
<script src="{{ asset('js/back-end/progressbar/bootstrap-progressbar.min.js') }}"></script>
<script src="{{ asset('js/back-end/nicescroll/jquery.nicescroll.min.js') }}"></script>
<!-- icheck -->
<script src="{{ asset('js/back-end/icheck/icheck.min.js') }}"></script>
<!-- daterangepicker -->
<script type="text/javascript" src="{{ asset('js/back-end/moment/moment.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('js/back-end/datepicker/daterangepicker.js') }}"></script>
<!-- chart js -->
<script src="{{ asset('js/back-end/chartjs/chart.min.js') }}"></script>

<script src="{{ asset('js/back-end/custom.js') }}"></script>

{% block flot_js %}<!-- flot js -->{% endblock %}

{% block worldmap %}<!-- worldmap -->{% endblock %}

{% block skycons %}<!-- skycons -->{% endblock %}

{% block dashbord_linegraph %}<!-- dashbord linegraph -->{% endblock %}

{% block datepicker %}<!-- datepicker -->{% endblock %}

{% block profilejs %}{% endblock %}

<!-- /footer content -->
</body>


</html>